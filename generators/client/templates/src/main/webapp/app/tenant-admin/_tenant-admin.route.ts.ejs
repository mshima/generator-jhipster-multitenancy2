import { RouterModule, Routes } from '@angular/router';
import { ModuleWithProviders } from '@angular/core';

import { UserRouteAccessService } from 'app/core/auth/user-route-access-service';

export const <%= tenantNameLowerFirst %>AdminState: Routes = [
  {
    path: 'user-management',
    data: {
      authorities: ['ROLE_<%= tenantNameUpperCase %>_ADMIN']
    },
    canActivate: [UserRouteAccessService],
    loadChildren: () => import('../admin/user-management/user-management.module').then(m => m.UserManagementModule)
  }
];

export const <%= tenantNameUpperFirst %>AdminRouting: ModuleWithProviders = RouterModule.forChild(<%= tenantNameLowerFirst %>AdminState);
