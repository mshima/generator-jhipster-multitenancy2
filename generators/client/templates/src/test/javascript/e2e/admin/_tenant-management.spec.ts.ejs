import { browser, ExpectedConditions as ec, promise } from 'protractor';
import { NavBarPage, SignInPage } from '../page-objects/jhi-page-objects';

import { <%= tenantNameUpperFirst %>MgmtComponentsPage, <%= tenantNameUpperFirst %>MgmtUpdatePage } from './<%= tenantNameLowerFirst %>-management.page-object';

const expect = chai.expect;

describe('<%= tenantNameLowerFirst %>-management e2e test', () => {

    let navBarPage: NavBarPage;
    let signInPage: SignInPage;
    let <%= tenantNameLowerFirst %>MgmtUpdatePage: <%= tenantNameUpperFirst %>MgmtUpdatePage;
    let <%= tenantNameLowerFirst %>MgmtComponentsPage: <%= tenantNameUpperFirst %>MgmtComponentsPage;

    before(async () => {
        await browser.get('/');
        navBarPage = new NavBarPage(true);
        signInPage = await navBarPage.getSignInPage();
        await signInPage.autoSignInUsing('admin', 'admin');
        await browser.wait(ec.visibilityOf(navBarPage.adminMenu), 5000);
    });

    it('should load <%= tenantNameLowerFirst %> management', async () => {
        await navBarPage.clickOnAdminMenu();
        await navBarPage.clickOnAdmin('<%= tenantNameLowerFirst %>-management');
        <%= tenantNameLowerFirst %>MgmtComponentsPage = new <%= tenantNameUpperFirst %>MgmtComponentsPage();
        await browser.wait(ec.visibilityOf(<%= tenantNameLowerFirst %>MgmtComponentsPage.title), 5000);
        expect(await <%= tenantNameLowerFirst %>MgmtComponentsPage.getTitle()).to.eq('<%= tenantNameLowerFirst %>Management.home.title');
    });

    it('should load create <%= tenantNameLowerFirst %> management page', async () => {
        await <%= tenantNameLowerFirst %>MgmtComponentsPage.clickOnCreateButton();
        <%= tenantNameLowerFirst %>MgmtUpdatePage = new <%= tenantNameUpperFirst %>MgmtUpdatePage();
        expect(await <%= tenantNameLowerFirst %>MgmtUpdatePage.getPageTitle()).to.eq('<%= tenantNameLowerFirst %>Management.home.createOrEditLabel');
        expect(await browser.getCurrentUrl()).to.match(new RegExp('.*/admin/<%= tenantNameLowerFirst %>-management/new'));
        await <%= tenantNameLowerFirst %>MgmtUpdatePage.cancel();
    });

    it('should create and save <%= tenantNameLowerFirst %> management', async () => {
        const nbButtonsBeforeCreate = await <%= tenantNameLowerFirst %>MgmtComponentsPage.countDeleteButtons();

        await <%= tenantNameLowerFirst %>MgmtComponentsPage.clickOnCreateButton();
        await promise.all([<%= tenantNameLowerFirst %>MgmtUpdatePage.setNameInput('new <%= tenantNameLowerFirst %>')]);
        expect(await <%= tenantNameLowerFirst %>MgmtUpdatePage.getNameInput()).to.eq('new <%= tenantNameLowerFirst %>', 'Expected Name value to be equals to name');
        await <%= tenantNameLowerFirst %>MgmtUpdatePage.save();
        expect(await <%= tenantNameLowerFirst %>MgmtUpdatePage.getSaveButton().isPresent(), 'Expected save button disappear').to.be.false;

        expect(await <%= tenantNameLowerFirst %>MgmtComponentsPage.countDeleteButtons()).to.eq(nbButtonsBeforeCreate + 1, 'Expected one more entry in the table');
        expect(await browser.getCurrentUrl()).to.match(new RegExp('.*/admin/<%= tenantNameLowerFirst %>-management'));
    });

    after(async () => {
        await navBarPage.autoSignOut();
    });
});
